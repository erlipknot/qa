            <tr class= "title_ticket">
              <th width="85%" class="title_content">&nbsp</th>
              <td width="5%" class="title_content">Y</td>
              <td width="5%" class="title_content">N</td>
              <td width="5%" class="title_content">n/a</td>
            </tr>
            <tr>
              <th>Issue was fully addressed</th>
              <td><input type="radio" id="tl_question_13" name="tl_add" value="5_tl_yes"></td>
              <td><input type="radio" id="tl_question_14" name="tl_add" value="5_tl_no"></td>
              <td><input type="radio" id="tl_question_15" name="tl_add" value="5_tl_na"></td>
