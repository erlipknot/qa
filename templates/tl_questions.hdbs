              <td><input type="radio" id="tl_question_18" name="tl_str" value="6_tl_na"></td>
            </tr>

            <!--<tr>
              <th>Anticipated future customer needs and additional information was provided</th>
              <td><input type="radio" id="tl_question_19" name="tl_inf" value="7_tl_yes"></td>
              <td><input type="radio" id="tl_question_20" name="tl_inf" value="7_tl_no"></td>
              <td><input type="radio" id="tl_question_21" name="tl_inf" value="7_tl_na"></td>
            </tr>-->
          </table>
